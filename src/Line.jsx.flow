/* @flow */
import * as React from 'react';

type Props<T> = {
  as: string | React.ComponentType<T>,
  theme: {
    baseFont: number,
    baseLineRatio: number,
  },
  fontSize: number,
  marginTop: number,
  marginBottom: number,
  border?: number,
}

type Line<T> = React.ComponentType<Props<T>>;

declare export default Line<*>;
